{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "_id": {
      "type": "string"
    },
    "icon": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "thumbnail": {
      "type": ["string", "null"]
    },
    "map": {
      "type": "string"
    },
    "svgXml": {
      "type": "string"
    },
    "visibility": {
      "type": "string"
    },
    "trashed": {
      "type": "boolean"
    }
  },
  "required": ["name"]
}
